<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tgs_layout_menu_group_item"
    android:layout_width="match_parent"
    android:layout_height="55dp"
    android:background="@color/white">

    <TextView
        android:id="@+id/tgs_textview_menu_group_item_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="메뉴 그룹명"
        android:textSize="16sp"
        android:textColor="@color/tgsfw_side_menu_step1_off_text"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:id="@+id/tgs_image_menu_group_item_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/gnb_d1_ic1"
        android:layout_marginRight="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>