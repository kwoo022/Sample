<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <com.android.tgsmf.view.webview.TGSWebview
        android:id="@+id/tgs_webview_dialog_web"
        android:layout_width="match_parent"
        android:layout_height="370dp"
        android:background="@color/white"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:weightSum="2">
        <Button
            android:id="@+id/tgs_button_web_dialog_negative"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/dialog_button_ok"
            android:textColor="@color/tgsfw_alert_dialog_button_text"
            android:textSize="13sp"
            android:background="#8b9baf"/>
        <Button
            android:id="@+id/tgs_button_web_dialog_positive"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/dialog_button_cancel"
            android:textColor="@color/tgsfw_alert_dialog_button_text"
            android:textSize="13sp"
            android:background="#4066f6"/>
    </LinearLayout>

</LinearLayout>