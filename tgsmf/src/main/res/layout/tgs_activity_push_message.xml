<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.TGSPushMessageActivity">
    <include
        android:id="@+id/layout_title_bar"
        layout="@layout/tgs_title_bar_back"
        app:layout_constraintTop_toTopOf="parent"/>
    <ExpandableListView
        android:id="@+id/tgs_listview_push_message"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:divider="@null"
        android:groupIndicator="@drawable/selector_indicator_push_message"
        app:layout_constraintTop_toBottomOf="@id/layout_title_bar"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>